<UserControl
    x:Class="ASOFTCIM.MVVM.View.Config.Test"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ASOFTCIM.MVVM.View.Config"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Button x:Name="btnTest" Content="TestPLC2CIM_SEND" />
        <TextBox x:Name="txtNameClass" Grid.Column="1" />
        <TextBox x:Name="txtNameObj" Grid.Column="2" />
        <!--  =  -->
        <Button
            x:Name="btnS2F17"
            Grid.Row="1"
            Grid.Column="0"
            Content="SendS2F17" />
        <!--  =  -->
        <Button
            x:Name="btnS1F1"
            Grid.Row="2"
            Grid.Column="0"
            Content="SendS1F1" />

    </Grid>
</UserControl>
